<div class="container">
        <text style="font-size: 12px;">
            <span>{{ title }}</span>
        </text>
        <div class="input">
            <input type="number" value="{{ code }}" class="input" placeholder="请输入" onchange="onInputCode" ></input>
        </div>
        <div class="buttons">
            <button class="buttons-item" @click="back">取消</button>
            <button class="buttons-item" @click="confirm">确认</button>
        </div>
</div>
