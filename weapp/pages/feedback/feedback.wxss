/* pages/feedback/feedback.wxss */
.feedback {
  padding: 24rpx;
}

.feedback-tip {
  color: #666;
  font-size: 28rpx;
}

.feedback-input {
  border: 1rpx solid #e5e5e5;
  display: block;
  border-radius: 8rpx;
  margin: 0 auto;
  padding: 16rpx 24rpx;
  width: 100%;
  margin-top: 24rpx;
}

.feedback-button {
  background-color: #1989fa;
  height: 120rpx;
}
